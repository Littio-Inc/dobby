---
import DashboardLayout from "../layouts/DashboardLayout.astro";
import AuthGuard from "../components/atoms/auth-guard.vue";
import MonetizationContainer from "../components/organisms/monetization-container.vue";

const lang = "es";
const title = "Dobby - Monetización";
---
<DashboardLayout title={title} lang={lang} pageTitle="Monetización">
  <AuthGuard client:load>
    <div class="p-8">
      <MonetizationContainer lang={lang} client:load />
    </div>
  </AuthGuard>
</DashboardLayout>
