---
import DashboardLayout from "../layouts/DashboardLayout.astro";
import AuthGuard from "../components/atoms/auth-guard.vue";
import BackOfficeMovementsContainer from "../components/organisms/back-office-movements-container.vue";

const lang = "es";
const title = "HUB - Movimientos Internos";
---
<DashboardLayout title={title} lang={lang} pageTitle="">
